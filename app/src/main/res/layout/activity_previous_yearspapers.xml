<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".cardviewActivities.previousYearspapers">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="378dp"
        android:layout_height="104dp"
        android:scaleY="1.3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/backpaper" />

    <TextView
        android:id="@+id/pcmToggle"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:background="@drawable/pcmbackchange"
        android:fontFamily="@font/lemonbold"
        android:gravity="center"
        android:text="PCM"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.291"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pcbToggle"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginTop="43dp"
        android:background="@drawable/pcmback"
        android:fontFamily="@font/lemonbold"
        android:gravity="center"
        android:text="PCB"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/pcmToggle"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.292"
        app:layout_constraintStart_toEndOf="@+id/pcmToggle"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="1.0" />

    <FrameLayout
        android:id="@+id/frame1"
        android:layout_width="393dp"
        android:layout_height="562dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.461">

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>